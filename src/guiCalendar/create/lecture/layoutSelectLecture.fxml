<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import input.elements.combobox.ComboBox?>
<?import javafx.scene.control.Button?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="selectLecture_grid" prefHeight="250.0" prefWidth="500.0"
          hgap="10" vgap="10" alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="guiCalendar.create.lecture.SelectLectureController">
    <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>

    <Text fx:id="sceneTitle" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2">
        <font><Font size="20"/></font>
    </Text>

    <ComboBox fx:id="lectureComboBox" GridPane.columnIndex="0" GridPane.rowIndex="1" prefWidth="200.0" prefHeight="65.0"/>
    <Button fx:id="select" onAction="#handleSelectButtonAction" GridPane.columnIndex="1" GridPane.rowIndex="1" prefWidth="125.0" prefHeight="65.0"/>

</GridPane>
