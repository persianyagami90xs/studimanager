<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="welcome_grid" prefHeight="300.0" prefWidth="450.0"
          hgap="10" vgap="10" alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="guiCalendar.welcome_screen.ControllerWelcomeScreen">
    <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>

    <Text fx:id="sceneTitle" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2">
        <font><Font size="20"/></font>
    </Text>

    <Label fx:id="newTimetableTitle" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

    <Label fx:id="importTimetableTitle" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <Button fx:id="importTimetableButton" onAction="#handleImportTimetableButtonAction" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
</GridPane>
